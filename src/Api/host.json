{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "default": "Debug",
      "Function": "Debug",
      "Host.Results": "Debug",
      "Host.Aggregator": "Debug",
      "Host.Startup": "Debug"
    }
  }
}
